	pf(summary(lm_yini_pc5)$`Response M48`$fstatistic[1],summary(lm_yini_pc5)$`Response M48`$fstatistic[2],summary(lm_yini_pc5)$`Response M48`$fstatistic[3],lower.tail=F),
