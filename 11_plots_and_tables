#===================Yields surface=======================

yields_daily_100_yr %>%
data.matrix() %>% 
t() %>%
plot_ly(
x=dates_daily$`as.Date(yields_daily_chrctr$DATE, "%d - %m - %Y")`,
y=c(0:121),
z=.,
type="surface"
) %>%
plotly::layout(
scene=list(
xaxis=list(title="Data"),
yaxis=list(title="Termo (meses)"),
zaxis=list(title="Taxa (% a.a.)")))

#===================diebold figure 1.2=======================

plot(yields_clmn$DATE,                          
     yields_clmn$M2*1200,
     type = "l",
     col = 2,
