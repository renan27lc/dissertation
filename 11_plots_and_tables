ar1_r1mo_M2<-lm(r1mo_M2M96_100[,1] ~ Lag(r1mo_M2M96_100[,1],1))
ar1_r1mo_M3<-lm(r1mo_M2M96_100[,2] ~ Lag(r1mo_M2M96_100[,2],1))
